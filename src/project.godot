; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Blacken Slash"
run/main_scene="res://components/main/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="blackenslash"
boot_splash/image="res://assets/splash.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://assets/app_icons/linux/512x512.png"
config/macos_native_icon="res://assets/app_icons/mac/icon.icns"
config/windows_native_icon="res://assets/app_icons/win/icon.ico"
config/quit_on_go_back=false

[autoload]

SteamAPI="*res://scripts/steam_handler.gd"
Config="*res://Config.gd"
Pathfinding="*res://scripts/pathfinding.gd"
Util="*res://scripts/util.gd"
RNG="*res://scripts/rng.gd"
State="*res://scripts/state.gd"
SFX="*res://scripts/sfx.gd"
Music="*res://scripts/music.gd"
Settings="*res://scripts/settings.gd"
Levels="*res://scripts/levels.gd"
Controls="*res://scripts/controls.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=700
window/size/height=700
window/size/fullscreen=true
window/size/test_width=700
window/size/test_height=700
window/vsync/vsync_via_compositor=true
window/handheld/orientation="portrait"
mouse_cursor/custom_image="res://assets/cursor.png"
window/stretch/mode="2d"
window/stretch/aspect="expand"
window/size/width.pc=900
window/size/height.pc=900

[global]

touch=false

[gui]

common/snap_controls_to_pixels=false

